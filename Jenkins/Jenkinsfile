pipeline {
    agent any
    stages {
        stage('Generate Folder Structures using Config Builder') {
            steps {
                bat '''
                CD D:\\IFS_J\\Build && RMDIR /S /Q
                CD C:\\Program Files (x86)\\IFS\\IFSConfigBuilder10
                IfsConfigBuilder .\\setup.ini .\\log.txt
                type log.txt'''
                echo 'Generate Folder Structure is executed.'
            }
        }
    }
}
